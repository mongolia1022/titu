DROP TABLE IF EXISTS `qyb_outdoor_sos`;
CREATE TABLE `qyb_outdoor_sos` (
  `id` int(11) NOT NULL auto_increment,
  `user_id` int(11) NOT NULL,
  `gps_x` float(8,5) NOT NULL,
  `gps_y` float(8,5) NOT NULL,
  `datetime` datetime NOT NULL,
  `timestamp` int(11) NOT NULL,
  PRIMARY KEY  (`id`),
  KEY `user_id` (`user_id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;